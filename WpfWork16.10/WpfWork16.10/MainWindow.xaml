<Window x:Class="WpfWork16._10.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfWork16._10"
        mc:Ignorable="d"
        Title="MainWindow" Height="330" Width="285.666">
    <Grid>
        <TextBlock x:Name="loginTextBlock" HorizontalAlignment="Left" Margin="10,101,0,0" TextWrapping="Wrap" Text="Login" VerticalAlignment="Top"/>
        <TextBlock x:Name="passwordTextBlock" HorizontalAlignment="Left" Margin="10,136,0,0" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top"/>
        <TextBox x:Name="loginTextBox" HorizontalAlignment="Left" Height="17" Margin="82,103,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="passwordTextBox" HorizontalAlignment="Left" Height="17" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Margin="82,138,0,0"/>
        <Button x:Name="signUpButton" Content="Sign Up" HorizontalAlignment="Left" Margin="102,172,0,0" VerticalAlignment="Top" Width="75" Click="SignUpButton_Click"/>
        <Button x:Name="checkedUser" Content="Check user" HorizontalAlignment="Left" Margin="102,249,0,0" VerticalAlignment="Top" Width="75" Click="CheckUserButton_Click"/>

    </Grid>
</Window>
